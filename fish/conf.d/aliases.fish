alias lllinux 'exa --long --all --time-style=long-iso --no-user --classify'
alias ll 'eza --long --all --time-style=long-iso --no-user --classify'
alias python python3
alias vim nvim

alias gitpfwl 'git push --force-with-lease'
alias gitca 'git commit --amend'
alias gitccblinux 'git branch --show-current | xclip -selection clipboard -rmlastnl'
alias gitccb 'git branch --show-current | pbcopy'
alias gitlog 'git log --oneline --graph --all --decorate'
alias gitdiff 'git difftool -y'
alias gitst 'git status -s'
